<?xml version="1.0"?>
<!-- This is the main urdf document, calling other xacro, including -->
<!-- rielbot_platform:  main platform -->
<!-- rielbot_gazebo:    gazebo simulation parameters for the main platform -->

<robot name="rielbot"
    xmlns:xacro="http://ros.org/wiki/xacro">

    <xacro:include filename="$(find rielbot_description)/urdf/rielbot_platform.xacro" />
    <xacro:include filename="$(find rielbot_description)/urdf/rielbot_gazebo.xacro" />

</robot>